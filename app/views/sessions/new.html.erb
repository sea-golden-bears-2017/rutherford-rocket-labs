<h1>Welcome to Rutherford Rocket Labs</h1>

<%= render partial: "errors", locals: {errors: @errors } %>
<%= form_tag(sessions_path, method: "post") do %>
  <%= label_tag 'username' %>
  <%= text_field_tag "username" %>
  <%= label_tag 'password' %>
  <%= password_field_tag "password" %>
  <%= submit_tag "login"%>
<% end %>
